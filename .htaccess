RewriteEngine On
#ErrorDocument 404 /noPage.php

RewriteRule ^home$ index.php
RewriteRule ^marAdentro$ marAdentro.php
RewriteRule ^galeria$ galeria.php
RewriteRule ^proyectos$ proyectos.php
RewriteRule ^amenities$ amenities.php
RewriteRule ^contacto$ contacto.php
RewriteRule ^ubicacion$ ubicacion.php
RewriteRule ^gracias$ contacto.php?gracias
RewriteRule ^crystalLagoons$ crystalLagoons.php
RewriteRule ^lotesMarAdentro$ lotes.php
RewriteRule ^gestionDeTierras$ lotes.php

RewriteRule ^reportes$ reportes.php
RewriteRule ^reporteContacto$ reportes.php?r=1
RewriteRule ^reporteLotes$ reportes.php?r=2
RewriteRule ^reporteConcurso$ reportes.php?r=3

RewriteRule ^proyectos/neiva$ proyectosNeiva.php
RewriteRule ^proyectos/cartagena$ proyectosCartagena.php
RewriteRule ^proyectos/flandes$ proyectosFlandes.php
RewriteRule ^amenities/neiva$ amenitiesNeiva.php
RewriteRule ^concursoNioMexico$ formEstandar.php

<IfModule mod_deflate.c>
<FilesMatch ".(js|jpg|jpeg|gif|png|css|txt|html)$">
ExpiresActive on
ExpiresDefault "access plus 1 month"
SetOutputFilter DEFLATE
</FilesMatch>
</IfModule>